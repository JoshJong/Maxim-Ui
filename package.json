  "nodejs": true,
  "main": "index.html",
  "node-remote": "http://localhost:3030",
  "node-main": "app.js",
  "single-instance": true,
  "window": {
    "frame": false,
    "toolbar": false,
    "title": "node-webkit demo",
    "icon": "maxim-logo.png",
    "position": "center",
    "resizable": true,
    "show_in_taskbar": true,
    "fullscreen": false,
    "always-on-top": false,
